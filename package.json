{
  "name": "portfolio-v3",
  "version": "1.0.0",
  "description": "the third iteration of my personal portfolio",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server/server",
    "server": "nodemon server/server",
    "client": "npm start --prefix client",
